<html>
  <head>
    <title>{{ title }}</title>
    <meta name="description" content="{{ description }}">
    <link rel="stylesheet" href="_assets/css/style.css">
  </head>

  <body>
    {{{ contents }}}

    {{#each collections.projects}}
      <h3><a href="{{this.url}}">{{this.title}}</a></h3>
      <article>{{{this.description}}}</article>
    {{/each}}

    {{#each collections.texts}}
      <h3><a href="{{this.url}}">{{this.title}}</a></h3>
      <article>{{{this.description}}}</article>
    {{/each}}

  </body>
</html>